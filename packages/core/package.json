{
  "name": "@agentflow/core",
  "version": "0.1.0-alpha2",
  "description": "AgentFlow compiler and execution engine, enabling AI-powered workflows and automations using Markdown and natural language.",
  "type": "module",
  "homepage": "https://github.com/lebrunel/agentflow",
  "repository": {
    "type": "git",
    "url": "https://github.com/lebrunel/agentflow",
    "directory": "packages/core"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "browser": "./dist/index.min.js",
  "unpkg": "./dist/index.min.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/* && bun run build:tsc && bun run build:web",
    "build:tsc": "tsc --p tsconfig.build.json",
    "build:web": "bun run build.ts"
  },
  "dependencies": {
    "ai": "^3.4.3",
    "change-case": "^5.4.4",
    "estree-walker": "^3.0.3",
    "it-pushable": "^3.2.3",
    "mdast-util-mdx-jsx": "^3.1.2",
    "mdast-util-to-string": "^4.0.0",
    "nanoevents": "^9.0.0",
    "polyfill-text-decoder-stream": "^0.0.9",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "ts-dedent": "^2.2.0",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-map": "^4.0.0",
    "unist-util-select": "^5.1.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "vfile": "^6.0.2",
    "yaml": "^2.5.0",
    "zod": "^3.23.8"
  }
}
